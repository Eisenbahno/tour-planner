@using MudBlazor

<MudDialog>
    <DialogContent>
        <MudText Typo="Typo.h6">Tour Report successfully Generated!</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="@Close" Color="Color.Primary">OK</MudButton>
    </DialogActions>
</MudDialog>

@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }

    private void Close()
    {
        MudDialog.Close(DialogResult.Ok(true));
    }

}